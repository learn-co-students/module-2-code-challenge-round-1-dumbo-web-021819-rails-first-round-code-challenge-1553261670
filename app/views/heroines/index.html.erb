<h2>All Heroines</h2>

<h2>Search by Superpower</h2>
<%= form_for "/search", :method => :get do |f|%>
<%=  f.text_field :power %>
<%= f.submit %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%=link_to heroine.super_name, heroine_path(heroine.id)%>
    </li>
    <br>
  <% end %>
</ul>
<%= link_to "Add new Heroine", new_heroine_path %>

